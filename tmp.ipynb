    "class MyClass:\n",
    "    def __init__(self, name):\n",
    "        self.name = name\n",
    "\n",
    "    def __del__(self):\n",
    "        print(f\"{self.name} is being destroyed\")\n",
    "\n",
    "# 创建对象\n",
    "obj = MyClass(\"example\")\n",
    "\n",
    "# 删除对象引用\n",
    "del obj\n",
    "\n",
    "# 强制垃圾回收（可选）\n",
    "import gc\n",
    "gc.collect()"
   ]
